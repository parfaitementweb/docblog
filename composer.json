{
    "name": "parfaitementweb/docblog",
    "description": ":description",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexis",
            "homepage": "https://www.parfaitementweb.com"
        }
    ],
    "homepage": "https://github.com/parfaitementweb/docblog",
    "keywords": ["Laravel", "Blog", "Documentation", "Package"],
    "require": {
        "illuminate/support": "~5",
        "laracasts/flash": "^3.0",
        "davejamesmiller/laravel-breadcrumbs": "5.x",
        "mcamara/laravel-localization": "^1.3",
        "spatie/laravel-feed": "^2.1",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-responsecache": "^4.4",
        "spatie/laravel-sluggable": "^2.1",
        "spatie/laravel-tags": "^2.1",
        "spatie/laravel-translatable": "^3.0",
        "teamtnt/laravel-scout-tntsearch-driver": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Parfaitementweb\\DocBlog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Parfaitementweb\\DocBlog\\Tests\\": "tests"
            "Parfaitementweb\\DocBlog\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Parfaitementweb\\DocBlog\\DocBlogServiceProvider"
            ],
            "aliases": {
                "DocBlog": "Parfaitementweb\\DocBlog\\Facades\\DocBlog"
            }
        }
    }
}
